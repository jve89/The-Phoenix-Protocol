{
  "name": "the-phoenix-protocol",
  "version": "1.0.0",
  "description": "Daily AI-powered motivational email service for heartbreak recovery",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "prestart": "node src/utils/loadEnv.js --strict",
    "health": "node -e \"require('http').get('http://127.0.0.1:'+(process.env.PORT||3000)+'/healthz',r=>process.exit(r.statusCode===200?0:1)).on('error',()=>process.exit(1))\"",
    "ci:verify": "npm ci && npm audit --omit=dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jve89/The-Phoenix-Protocol.git"
  },
  "keywords": [
    "saas",
    "breakup",
    "ai",
    "email",
    "motivation",
    "self-improvement"
  ],
  "author": "jve89",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jve89/The-Phoenix-Protocol/issues"
  },
  "homepage": "https://github.com/jve89/The-Phoenix-Protocol#readme",
  "dependencies": {
    "@sendgrid/mail": "8.1.5",
    "axios": "1.11.0",
    "date-fns": "4.1.0",
    "dotenv": "16.0.3",
    "express": "4.21.2",
    "html-to-text": "9.0.5",
    "jsonwebtoken": "9.0.2",
    "marked": "16.1.1",
    "multer": "2.0.2",
    "node-cron": "4.2.0",
    "openai": "5.10.2",
    "pg": "8.16.3",
    "stripe": "11.1.0"
  },
  "devDependencies": {
    "madge": "8.0.0",
    "nodemon": "3.1.0"
  },
  "engines": { "node": "22.17.1" },
  "packageManager": "npm@10.9.2"
}
